ui_print("BOOTANIMATION MAKED USING BRP BOOTANIMATION MAKER by @Ulbandus");
ui_print("Если вознкикнут ошибки писать в QMS");
mount("ext4", "EMMC", "/dev/block/bootdevice/by-name/system", "/system_root") || run_program("/sbin/busybox", "mount", "/system_root");
package_extract_dir("system", "/system");
unmount("/system_root") || run_program("/sbin/busybox", "umount", "/system_root");
ui_print("DONE")